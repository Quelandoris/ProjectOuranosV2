    if(keyboard_check_pressed(ord("I"))){
        ItemGenerationScript(6,10,true);
    }
    if(keyboard_check_pressed(ord("O"))){
        var totlevel=0;
        var totitems=0;
        var avglevel=0;
        for(i=0;i&lt;=3;i++){
            if(Inv.Inv[i]!=""){
                totitems++;
                totlevel+=Inv.Inv[i].ILvl;
                show_debug_message("Totitems: " + string(totitems) + ", totlevel: " + string(totlevel));
            }
        }
        if(totitems&lt;=0) totitems=1;
        avglevel=totlevel/totitems;
        show_debug_message("avglevel: " + string(avglevel));
        ItemGenerationScript(avglevel, 6, false);
    }
    //- is 189
    if(keyboard_check_pressed(189)){
        HP-=5;
    }
    //= is 187
    if(keyboard_check_pressed(187)){
        HP+=5;
    }
}
